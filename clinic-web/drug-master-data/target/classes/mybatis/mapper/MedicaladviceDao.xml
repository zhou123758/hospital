<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wotrd.data.dao.MedicaladviceDao">
    <select id="getMedicaladviceAll" resultType="com.wotrd.data.pojo.Medicaladvice">
        select * from medicaladvice
    </select>
    <insert id="medicaladviceSave">
        insert into medicaladvicejoin (nid, mid) values (#{nid},#{mid});
    </insert>
</mapper>